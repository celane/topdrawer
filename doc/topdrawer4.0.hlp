 [4mRE[0mAD [4mME[0mSH [[4mBI[0mNS] [[4mAP[0mPEND[=[4mON[0m|[4mOF[0mF]] [[4mWI[0mTH] [[4mER[0mRORS[=[4mON[0m|[4mOF[0mF]] 
      [[4mFO[0mR] {X|Y|Z}[=] N1,...,Nn [nn+1 [4mEN[0mDBINS] 
 [[4mFO[0mR] {X|Y|Z}[=]nn {X|Y|Z}[=] N1,...,Nn 
